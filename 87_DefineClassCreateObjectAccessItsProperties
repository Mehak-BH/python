class Student:
    def __init__(self, name, age):
        self.name = name
        self.age = age

    def show(self):
        print("Name:", self.name, "| Age:", self.age)

# Create objects
s1 = Student("Mehak", 19)
s2 = Student("Aarav", 18)

# Access properties
s1.show()
s2.show()

print("This code is written and excecuted by MEHAK BHUTANI with ERP 0231BCA063")
